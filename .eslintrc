{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "extends": [
        "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "semi": [2, "always"],
      "indent": ["error", 2],
      "no-console": "off",
      "no-unused-vars": "warn",
      "import/prefer-default-export": "off",
      "@typescript-eslint/no-unused-vars": "warn"
    }
}